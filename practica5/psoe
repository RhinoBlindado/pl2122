inicio
{
    inivar
        entero ve, ve2;
        real vf;
        caracter vc, v2, v4;
        booleano vl, vl2, vl3, vl4;
    finvar;

	ve = 4 * 3 ^ 2;
	vf = 3.5;

	si(Verdadero)
	{
		ve = 7;
		si(Verdadero)
		{
			ve = 1;
			vf = 4.33;
			vf = 7.44;
			si(Falso)
			{
				ve = 4444;
			}
		}
	}

	anunciar(ve, vf);
}
